    "    #print(query)\n",
    "    cursor.execute(query)\n",
    "    rows = cursor.fetchall()\n",
    "    df_name = i + \"_df\"\n",
    "    globals()[df_name]= pd.DataFrame(rows)\n",
    "    print(df_name)\n",
    "    \n",
