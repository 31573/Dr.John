    "    #csv for headers\n",
    "    path = f'data/{i}.csv'\n",
    "    csv_path = Path(path)\n",
    "    with open(csv_path, 'r', newline='') as file:\n",
    "        reader = csv.reader(file)\n",
    "        headers = next(reader)  # Read the first row\n",
    "        if len(globals()[df_name].columns) == len(headers):\n",
    "            globals()[df_name].columns = headers\n",
    "\n",
